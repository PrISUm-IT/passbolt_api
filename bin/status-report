#!/usr/bin/env bash
USER="$2"
if [ -z "$USER" ];
then
    USER="www-data"
fi

./bin/versions
sudo -H -u $USER bash -c "./bin/cake passbolt healthcheck"
sudo -H -u $USER bash -c "./bin/cake passbolt cleanup --dry-run"
sudo -H -u $USER bash -c "./bin/cake passbolt datacheck --hide-success-details"
cat ./logs/error.log
